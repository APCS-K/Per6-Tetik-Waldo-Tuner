    public static void changeVolumeByByte() {
        byte[] data = SplitUp();
        for (int i = 0; i < data.length; i*=10) {
            try	{
		byte[] byteData	= new byte[10];
		for (int r = 0; r < byteData.length; r++) {
                    byteData[r]	= data[r+i];
		}
                ByteArrayInputStream input = new ByteArrayInputStream(byteData);
                Clip clip= AudioSystem.getClip();
                AudioInputStream audioInputStream = new AudioInputStream(input, audioFormat, byteData.length);
            }
            catch (Exception e) {
                e.printStackTrace();
            }
        }
    }
    public static void main(String[] args) {
        changeVolume();
        byte[] data = SplitUp();
        System.out.println(data.length);
    }
}
